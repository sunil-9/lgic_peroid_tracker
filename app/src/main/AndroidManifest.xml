<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.periodtracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.Light.NoActionBar">
        <activity android:name="com.example.periodtracker.activity.SettingActivity"></activity>
        <activity android:name="com.example.periodtracker.activity.ProfileActivity" />
        <activity android:name="com.example.periodtracker.activity.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.periodtracker.activity.ForgetPasswordActivity" />
        <activity android:name="com.example.periodtracker.activity.SignupActivity" />
        <activity android:name="com.example.periodtracker.activity.LoginActivity" />
        <activity android:name="com.example.periodtracker.activity.MainActivity" />
    </application>

</manifest>